<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kosta.boot.board.domain.repository.mapper.BoardMapper">

    <insert id="save" parameterType="Board" useGeneratedKeys="true" keyProperty="no">
        INSERT INTO bootboard.Board(title, content, hit, reg_date, user_no)
        VALUES (
                #{title},
                #{content},
                #{hit},
                SYSDATE(),
                #{userNo})
    </insert>

</mapper>